{
  "manifest_version": 2,
  "author": "ogiexela",
  "name": "No New Tabs",
  "description": "Automatically close any new tabs or windows.",
  "version": "1.0",
  "icons": {
    "16": "icons/16.png",
    "20": "icons/20.png",
    "24": "icons/24.png",
    "25": "icons/25.png",
    "30": "icons/30.png",
    "32": "icons/32.png",
    "40": "icons/40.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png",
    "172": "icons/172.png",
    "512": "icons/512.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "*://*/*",
    "tabs",
    "activeTab",
    "background",
    "contextMenus"
  ]
}
